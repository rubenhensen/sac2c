@MACRO_NONULL@
@MACRO_NORETURN@
@MACRO_PURE@
@MACRO_CONST@
#endif

#ifdef NO_FUN_ATTRS
@MACRO_FORMAT_PRINTF_NONE@
@MACRO_UNUSED_NONE@
@MACRO_MALLOC_NONE@
@MACRO_NONULL_NONE@
@MACRO_NORETURN_NONE@
@MACRO_PURE_NONE@
@MACRO_CONST_NONE@
#endif

#endif // __FUN_ATTRS_H_
