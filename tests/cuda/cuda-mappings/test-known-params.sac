// SAC_TEST|include common.mk
// SAC_TEST|SAC2C_FLAGS += -t cuda -check g -gpukernel
// SAC_TEST|all: <file-name>

#include "mini-stdlib.sac"

noinline
int[16,16] hiding()
{
    a = with {
#pragma gpukernel
        GridBlock ( 2,
        PadLast(32,
        FoldLast2(
        Permute([1, 0, 2],
        SplitLast(2,
        CompressGrid([1,1],
        ShiftLB(
        Gen )))))))
        ([2,2] <= iv < [12,12]): 42;
    } : genarray( [16, 16], 0);
    return a;
}

int main()
{
    a = hiding();
    return a[[10, 10]];
}
